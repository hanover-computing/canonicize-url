{
  "name": "canonicize-url",
  "version": "1.5.4",
  "description": "Get a stable, canonical version of any URL!",
  "type": "module",
  "repository": "hanover-computing/normalize-url-plus",
  "author": "Jane Jeon <git@janejeon.com>",
  "license": "LGPL-3.0",
  "bin": "bin/normalize.js",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "update-data": "curl -L -s https://rules2.clearurls.xyz/data.minify.json > src/data/data.minify.json",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "cacheable-lookup": "^6.0.1",
    "cheerio": "^1.0.0-rc.10",
    "debug": "^4.3.2",
    "ipaddr.js": "^2.0.1",
    "leven": "^4.0.0",
    "lodash": "^4.17.21",
    "mem": "^9.0.1",
    "normalize-url": "^7.0.2",
    "p-timeout": "^5.0.0",
    "quick-lru": "^6.0.1",
    "re2": "^1.16.0",
    "tld-extract": "^2.0.1"
  },
  "devDependencies": {
    "@janejeon/eslint-config-typescript": "^0.1.0",
    "@janejeon/prettier-config": "^2.0.0",
    "@janejeon/tsconfig": "^0.3.1",
    "@types/debug": "^4.1.8",
    "@vitest/coverage-istanbul": "^0.34.2",
    "@vitest/ui": "^0.34.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "nock": "^13.1.3",
    "npm-run-all": "^4.1.5",
    "skip-ci": "^1.0.4",
    "type-fest": "^4.3.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  },
  "peerDependencies": {
    "got": "^12 || ^13"
  },
  "keywords": [
    "normalize",
    "url",
    "uri",
    "address",
    "string",
    "normalization",
    "normalisation",
    "query",
    "querystring",
    "simplify",
    "strip",
    "trim",
    "canonical"
  ]
}
